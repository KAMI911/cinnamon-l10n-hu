#: usr/lib/linuxmint/mintLocale/add.py:42
msgid "Add a New Language"
msgstr "Új nyelv hozzáadása"

#: usr/lib/linuxmint/mintLocale/install_remove.py:53
msgid "Install / Remove Languages"
msgstr "Nyelvek telepítése vagy eltávolítása"

#: usr/lib/linuxmint/mintLocale/install_remove.py:158
msgid "Some language packs are missing"
msgstr "Néhány nyelvi csomag hiányzik"

#: usr/lib/linuxmint/mintLocale/install_remove.py:162
msgid "Fully installed"
msgstr "Teljesen telepítve"

#: usr/lib/linuxmint/mintLocale/install_remove.py:196
#: usr/lib/linuxmint/mintLocale/install_remove.py:235
msgid "Please wait, this can take some time"
msgstr "Kis türelmet, ez eltarthat egy ideig"

#: usr/lib/linuxmint/mintLocale/install_remove.py:198
msgid "Installation is complete"
msgstr "A telepítés befejeződött"

#: usr/lib/linuxmint/mintLocale/install_remove.py:237
msgid "The related language packs were removed"
msgstr "A nyelvi csomagok eltávolításra kerültek"

#: usr/lib/linuxmint/mintLocale/mintLocale.py:46
msgid "Language Settings"
msgstr "Nyelvi beállítások"

#: usr/lib/linuxmint/mintLocale/mintLocale.py:120
msgid "No locale defined"
msgstr "Nincs területi beállítás"

#: usr/lib/linuxmint/mintLocale/add.ui.h:1
msgid "Install"
msgstr "Telepítés"

#: usr/lib/linuxmint/mintLocale/add.ui.h:2
msgid "Install the selected language"
msgstr "A kiválasztott nyelv telepítése"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:1
msgid ""
"<b><small>Note: Installing or upgrading language packs can trigger the "
"installation of additional languages</small></b>"
msgstr ""
"<b><small>Megjegyzés: Nyelvi csomagok telepítése vagy frissítése további "
"nyelvek hozzáadását vagy cseréjét válthatja ki</small></b>"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:2
msgid "Add..."
msgstr "Hozzáadás…"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:3
msgid "Add a new language"
msgstr "Új nyelv hozzáadása"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:4
msgid "Remove"
msgstr "Eltávolítás"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:5
msgid "Remove the selected language"
msgstr "Kiválasztott nyelv eltávolítása"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:6
msgid "Install language packs"
msgstr "Nyelvi csomagok telepítése"

#: usr/lib/linuxmint/mintLocale/install_remove.ui.h:7
msgid ""
"Install any missing language packs, translations files, dictionnaries for "
"the selected language"
msgstr ""
"Telepítse a hiányzó nyelvi csomagokat, fordításokat, szótárakat a "
"kiválasztott nyelvhez"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:1
msgid "Select your default session language:"
msgstr "Válassza ki a környezet alapértelmezett nyelvét:"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:2
msgid "Select the default language for your user session"
msgstr "Válassza ki az alapértelmezett nyelvet a felhasználói környezethez"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:3
msgid "<b><small>Changes take effect the next time you log in.</small></b>"
msgstr ""
"<b><small>A változtatások a következő bejelentkezés után lépnek "
"életbe</small></b>"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:4
msgid "Apply System-Wide"
msgstr "Alkalmazás rendszer szinten"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:5
msgid "Use the selected language as the default system language"
msgstr "A kiválasztott nyelv használata a rendszer alapértelmezettként"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:6
msgid "Install / Remove Languages..."
msgstr "Nyelvek telepítése vagy eltávolítása…"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:7
msgid "Install additional languages or remove installed languages"
msgstr "További nyelvek telepítése, illetve eltávolítása"

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:8
msgid "System language: "
msgstr "Rendszer nyelve: "

#: usr/lib/linuxmint/mintLocale/mintLocale.ui.h:9
msgid "Number of languages installed: "
msgstr "Telepített nyelvek száma: "

#: generate_desktop_files:24
msgid "Languages"
msgstr "Nyelvek"

#: generate_desktop_files:24
msgid "Language settings"
msgstr "Nyelvi beállítások"
